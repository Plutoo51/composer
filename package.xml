<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>composer</name>
  <version>0.2.0</version>
  <description>Composer provides orchestration utilites for ROS2 environment</description>
  <maintainer email="info@composiv.ai">composiv.ai</maintainer>
  <license>Eclipse Public License</license>
  
  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>
  <test_depend>ament_cmake_pytest</test_depend>
  

  <exec_depend>python3-docker</exec_depend>
  <exec_depend>python3-listgits</exec_depend>
  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
